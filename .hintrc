{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "style[scoped]"
        ]
      }
    ],
    "meta-viewport": "off"
  }
}